{
  "include": [
    "remix.env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".react-router/types/**/*"
  ],
  "compilerOptions": {
    "jsx": "react",
    "types": [
      "@react-router/node",
      "vite/client"
    ],
    "rootDirs": [".", "./.react-router/types"],
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "Bundler"
  }
}