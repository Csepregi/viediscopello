  "devDependencies": {
    "@playwright/test": "^1.39.0",
    "@remix-run/dev": "^2.2.0",
    "@remix-run/eslint-config": "^2.2.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/eslint": "^8.44.6",
